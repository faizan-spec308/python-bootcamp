student = ("Faizan",19, "Brunel")

print(student.count("Brunel"))
print(student.index("Brunel"))

print("**********************")

for x in student :
    print(x)


#Sets

dishes = {"bowl","Plate", "cup."}
print(dishes)
for food in dishes :
    print(food)     
